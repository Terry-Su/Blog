  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "args": [
        "server.ts",
        // "scripts/buildContentsOnOtherPlatforms.ts"
      ],
      "runtimeArgs": ["-r", "ts-node/register", "-r",
        "tsconfig-paths/register",],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "env": {
        "NODE_ENV": "development",
        // "DEV": "true",
        // "SKIP_WEBPACK_COMPILE": "true",
        "SIMPLE_MODE": "true",
        "TS_NODE_PROJECT": "tsconfig.node.json"
      },
      "sourceMaps": true
    }

  ]
}
